{
  "extends": [
    "airbnb",
    "plugin:prettier/recommended",
    "react-hooks",
    "eslint:recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "react/require-default-props": "warn"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": { "react": { "version": "detect" } }
}
